{
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "testcafe"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "plugin:testcafe/recommended"
  ],
  "globals": {
    "AmCharts": true,
    "Chart": true,
    "CodeMirror": true,
    "FusionCharts": true,
    "Handsontable": true,
    "Highcharts": true
  },
  "rules": {
    "padded-blocks": 0
  }
}
